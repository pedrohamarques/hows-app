{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@screens/*": ["./src/screens/*"],
      "@routes/*": ["./src/routes/*"],
      "@typings/*": ["./src/typings/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@services/*": ["./src/services/*"],
